{
  "name": "to-json-schema",
  "version": "0.1.1",
  "description": "Convert JS object to JSON Schema",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "prepublish": "npm test && npm run build",
    "lint": "eslint src/ test/",
    "test": "mocha --recursive",
    "test:coverage": "istanbul cover _mocha -- --recursive",
    "test:coveralls": "istanbul cover _mocha -- --recursive && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruzicka/to-json-schema.git"
  },
  "keywords": [
    "JSON",
    "schema",
    "JSON schema",
    "convert"
  ],
  "author": "David Ruzicka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruzicka/to-json-schema/issues"
  },
  "homepage": "https://github.com/ruzicka/to-json-schema#readme",
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "lodash.keys": "^4.2.0",
    "lodash.merge": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.without": "^4.4.0",
    "lodash.xor": "^4.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "coveralls": "^2.13.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.5",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^2.5.3"
  },
  "files": [
    "lib"
  ]
